{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true,
      "regex": "\\d{4}-\\d{2}-\\d{2}-[\\dA-Za-z-]+"
    },
    "collection": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::collection.collection",
      "inversedBy": "blogs"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "thought",
        "article",
        "tutorial"
      ],
      "required": true,
      "default": "thought"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "content": {
      "type": "customField",
      "options": {
        "preset": "rich"
      },
      "customField": "plugin::ckeditor.CKEditor"
    }
  }
}
